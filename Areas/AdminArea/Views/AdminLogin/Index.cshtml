
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/AdminArea/Views/Shared/_AdminLayout.cshtml";
}

@model Voyager.Models.Vm.AdminUserVM

<!DOCTYPE html>
<html>
<head>
    <title></title>
</head>
<body>
    <form action="/Areas/AdminArea/AdminLogin/Index" method="post"></form>

    <div>
        @Html.LabelFor(q => q.EMail)
        @Html.TextBoxFor(q => q.EMail)
    </div>
    <div>
        @Html.LabelFor(q => q.Password)
        @Html.PasswordFor(q => q.Password)
    </div>
    <div>
        <input type="submit" name="name" value="Sign In" />
    </div>
    

</body>
</html>
